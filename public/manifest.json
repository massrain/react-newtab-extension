{
  "name": "Sarget - Sekme",
  "version": "0.0.2",
  "description": "Yeni sekme.",
  "author": "Emre A",
  "manifest_version": 2,
  "icons": {
    "16": "icons/16.png",
    "32": "icons/32.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "background": {
    "scripts": ["assets/addiyos.js"],
    "persistent": false
  },
  "permissions": ["storage", "tabs", "bookmarks", "history", "topSites", "chrome://favicon/", "geolocation"],
  "browser_action": {
    "default_icon": {
      "16": "icons/16.png",
      "32": "icons/32.png"
    },
    "default_title": "Replace new tab"
  },
  "chrome_settings_overrides": {
    "search_provider": {
        "name": "Addiyos",
        "keyword": "Addiyos custom search",
        "search_url": "http://addiyos.com/search?q={searchTerms}",
        "favicon_url": "http://addiyos.com/favicon.ico",
        "suggest_url": "ttp://addiyos.com/suggest?q={searchTerms}",
        "instant_url": "ttp://addiyos.com/instant?q={searchTerms}",
        "image_url": "ttp://addiyos.com/image?q={searchTerms}",
        "search_url_post_params": "search_lang=tr",
        "suggest_url_post_params": "suggest_lang=tr",
        "instant_url_post_params": "instant_lang=tr",
        "image_url_post_params": "image_lang=tr",
        "encoding": "UTF-8",
        "is_default": true
    }
   }
}
